--- for commits before hollowbone theme is added ---

using Colors

const hollowbone = Lamination.LaminationTheme(
  RGB(172/255, 146/255, 122/255), # leafcolor
  nothing,                        # fillcolor
  Lamination.SOLID,               # fillstyle
  nothing,                        # checkcolor
  RGB(39/255, 36/255, 33/255)     # diskcolor
)

